<UserControl x:Class="SturmProjekt.Views.ProfileControlView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"        
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             xmlns:custom="clr-namespace:SturmProjekt.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel>
            <Button Margin="0,10,0,10" Padding="3, 10, 30, 10" Command="{Binding LeftCommand}" Content="Vorige Page"/>
            <Button Margin="0,10,0,10" Padding="3, 10, 30, 10" Command="{Binding RightCommand}" Content="Nächste Page"/>
        </StackPanel>
    </Grid>
</UserControl>